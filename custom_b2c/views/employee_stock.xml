<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="hr_employee_view_form_inherit" model="ir.ui.view">
            <field name="name">stock.warehouse.form.inherit</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <notebook>
                    <page name="hr_empoyee" string="Nhân Viên kho">
                        <div id="o_work_employee_container">
                            <div id="o_work_employee_main">

                                <field name="employee" widget="many2many_tags"
                                       options="{'no_open': True, 'no_create': True , 'no_quick_create': True}"/>
                            </div>
                        </div>
                    </page>
                </notebook>
            </field>
        </record>
    </data>
</odoo>

